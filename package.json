{
  "name": "molecule",
  "version": "0.9.0",
  "description": "A Web IDE UI Framework built by React.js, inspired by VSCode.",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "dev": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook",
    "check-types": "tsc --skipLibCheck",
    "build": "tsc --project tsconfig.build.json",
    "eslint": "npx eslint ./src/**/*.ts ./src/**/*.tsx",
    "style-lint": "npx stylelint ./src/**/*.scss",
    "release": "standard-version"
  },
  "keywords": [
    "react.js",
    "vscode",
    "ui",
    "ide"
  ],
  "author": "Ziv",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^16.9.35",
    "classnames": "^2.2.6",
    "dt-react-monaco-editor": "^0.0.6",
    "dt-utils": "^1.0.1",
    "loadsh": "^0.0.4",
    "rc-collapse": "^2.0.1",
    "rc-tree": "^3.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-split-pane": "^0.1.92",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0",
    "vscode-codicons": "^0.0.10"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "6.0.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "6.0.21",
    "@storybook/addons": "6.0.21",
    "@storybook/react": "6.0.21",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "babel-loader": "8.0.6",
    "css-loader": "^4.3.0",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.3.0",
    "jest": "^26.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "standard-version": "^9.0.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
